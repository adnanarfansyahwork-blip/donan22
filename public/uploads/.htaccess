<FilesMatch "\.(php|php[0-9]?|phtml|phps|pht|phar)$">
    Require all denied
</FilesMatch>
<FilesMatch "\.(exe|com|bat|cmd|sh|cgi|pl|py)$">
    Require all denied
</FilesMatch>
<FilesMatch "\.(asp|aspx|jsp|rb)$">
    Require all denied
</FilesMatch>
Options -ExecCGI -Indexes
<IfModule mod_php.c>
    php_flag engine off
</IfModule>
<IfModule mod_php7.c>
    php_flag engine off
</IfModule>
<IfModule mod_php8.c>
    php_flag engine off
</IfModule>
<FilesMatch "\.(jpg|jpeg|png|gif|webp|svg|pdf|txt|doc|docx|xls|xlsx|zip|rar)$">
    Require all granted
</FilesMatch>